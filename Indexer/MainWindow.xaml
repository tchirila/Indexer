<Window x:Class="Indexer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        
        Title="IndexerWindow" Height="768" Width="1366">

    <Window.CommandBindings>
        <CommandBinding Command="Help" Executed="Btn_Execute"/>
    </Window.CommandBindings>

    <Grid Margin="-2,0,2,0" Background="LightGray">
        <StackPanel Orientation="Horizontal">
            <Button Command="Help" Margin="10" Width="50" Height="20" HorizontalAlignment="Left">Execute</Button>
            
        </StackPanel>
    </Grid>
</Window>
